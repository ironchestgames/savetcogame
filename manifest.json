{
	"manifest_version": 3,
	"name": "bananpermobil's copy TCO game stats",
	"description": "Copy selected TCO game stats into your data sheet",
	"version": "0.2",
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"64": "icon64.png"
  },
  "content_scripts": [
		{
			"matches": ["https://thecrucible.online/play"],
			"js": ["contentscript.js"]
		}
	],
	"action": {
    "default_popup": "popup.html"
  }
}
